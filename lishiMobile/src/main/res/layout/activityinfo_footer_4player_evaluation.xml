<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"

    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="15dp"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="精选玩家评价"
        android:textSize="17sp"
        android:textColor="#525252"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        />
    <View
        android:layout_width="36dp"
        android:layout_height="2dp"
        android:background="@drawable/title_line"
        android:layout_marginTop="15dp"
        android:layout_gravity="center_horizontal"
        />

    <RelativeLayout
        android:id="@+id/footer4playerevaluation_rl1"
        android:layout_width="match_parent"
        android:layout_height="37dp"
        android:layout_marginTop="20dp"
        >
        <com.lis99.mobile.club.widget.RoundedImageView
            android:id="@+id/footer4playerevaluation_player_head1_img"
            android:layout_width="37dp"
            android:layout_height="37dp"
            android:src="@drawable/v"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"

            app:corner_radius="14dp"
            />

        <LinearLayout
            android:id="@+id/footer4playerevaluation_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="14dp"
            android:layout_toRightOf="@id/footer4playerevaluation_player_head1_img"
            >
            <TextView
                android:id="@+id/footer4playerevaluation_player_name1_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="名字"
                android:textSize="14sp"
                android:textColor="#525252"
                android:layout_gravity="center_vertical"
                />
            <TextView
                android:id="@+id/footer4playerevaluation_player_label_11_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="标签1"
                android:textSize="9sp"
                android:maxLength="7"
                android:gravity="center"
                android:layout_marginLeft="4dp"
                android:textColor="#ffffff"
                android:background="@drawable/topic_supper_tag3"
                android:layout_gravity="center_vertical"
                />
            <TextView
                android:id="@+id/footer4playerevaluation_player_label_12_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="标签2"
                android:textSize="9sp"
                android:maxLength="7"
                android:layout_marginLeft="4dp"
                android:textColor="#ffffff"
                android:background="@drawable/topic_supper_tag3"
                android:layout_gravity="center_vertical"
                />
            <TextView
                android:id="@+id/footer4playerevaluation_player_label_13_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="标签3"
                android:textSize="9sp"
                android:maxLength="7"
                android:textColor="#ffffff"
                android:layout_marginLeft="4dp"
                android:background="@drawable/topic_supper_tag3"
                android:layout_gravity="center_vertical"
                />
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/footer4playerevaluation_rl2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_below="@id/footer4playerevaluation_ll"
            android:layout_toRightOf="@id/footer4playerevaluation_player_head1_img"
            >

            <RatingBar
                android:id="@+id/footer4playerevaluation_player_level_img"
                style="@style/ls_equip_star_reply"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="14dp"
                android:layout_centerVertical="true"
                android:isIndicator="true"
                android:numStars="5"
                android:rating="0"
                android:stepSize="0.5" />
            <TextView
                android:id="@+id/footer4playerevaluation_play_time1_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="2016.09.09 玩过"
                android:textSize="11sp"
                android:textColor="#999999"
                android:layout_alignParentRight="true"
                android:layout_marginRight="15dp"

                />
        </RelativeLayout>
        </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/footer4playerevaluation_player_evaluation_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="不错不错不错\n错不错\n错不错"
            android:textSize="14sp"
            android:textColor="#525252"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="51dp"
            android:layout_marginTop="14dp"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="51dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/fengexian"
        />
    </LinearLayout>

